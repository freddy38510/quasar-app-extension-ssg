{
  "name": "quasar-app-extension-ssg-monorepo",
  "private": true,
  "homepage": "https://github.com/freddy38510/quasar-app-extension-ssg",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freddy38510/quasar-app-extension-ssg.git"
  },
  "engines": {
    "node": ">= 16.14"
  },
  "scripts": {
    "release:ssg": "HUSKY=0 pnpm --filter quasar-app-extension-ssg run release",
    "generate:playground-vite": "pnpm -c --filter quasar-app-extension-ssg-test-vite exec quasar ssg generate",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.3",
    "@commitlint/config-conventional": "17.4.3",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "commit-and-tag-version": "11.1.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.34.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-lodash-template": "0.21.0",
    "husky": "8.0.3",
    "typescript": "4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "packageManager": "pnpm@8.5.1",
  "pnpm": {
    "overrides": {
      "quasar-app-extension-ssg": "workspace:*"
    }
  }
}
