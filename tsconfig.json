{
  "compilerOptions": {
    "baseUrl": "./",
    "lib": [
      "es2019",
      "es2020.promise",
      "es2020.bigint",
      "es2020.string"
    ],
    "paths": {
      "<%= quasarAppPkgName %>/*": [
        "node_modules/@quasar/app-webpack/*"
      ]
    },
    "module": "commonjs",
    "target": "es2019",
    "moduleResolution": "node",
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "isolatedModules": true,
    "useDefineForClassFields": true,
  },
}
